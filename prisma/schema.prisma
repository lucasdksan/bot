generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tasks {
  id          String   @id @db.Uuid
  title       String
  description String
  code        String   @unique
  estimated   DateTime

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  createdBy String
  updatedBy String

  @@map("tasks")
}
